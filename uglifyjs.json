{
  "module": true,
  "compress": {
    "passes": 3,
    "unsafe": true,
    "unsafe_comps": true,
    "unsafe_math": true,
    "unsafe_proto": true,
    "unsafe_regexp": true,
    "pure_getters": true,
    "reduce_funcs": true,
    "reduce_vars": true,
    "collapse_vars": true,
    "toplevel": true,
    "drop_console": true,
    "drop_debugger": true,
    "dead_code": true,
    "conditionals": true,
    "booleans": true,
    "sequences": true,
    "evaluate": true,
    "inline": true,
    "typeofs": true
  },
  "mangle": {
    "toplevel": true,
    "properties": {}
  },
  "output": {
    "comments": false,
    "beautify": false
  }
}
